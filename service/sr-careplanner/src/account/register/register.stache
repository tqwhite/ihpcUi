				<div class='termsNotification'>
				By clicking the Register button below, you agree to the Terms and Conditions displayed above. 
				<span class='inlineButton' style='color:rgba(255,255,255, .7)' ($click)='showModalTerms("#termsText", %event, "autoPrint")'>PRINT</span>

				</div>
			
			</div>
			
	</div>

<div class='buttonContainer'> 
		{{#browserLoaded}}
	<button class="c-button c-button--primary c-button--xsmall" ($click)='setNewPage("account", "login")'>Login</button>

		<button class="c-button c-button--primary c-button--small rightJustified" id="registerButton">Register</button>
</div>
		{{/browserLoaded}}
		
		{{^browserLoaded}}
		<span class="fa fa-spinner fa-spin basicBlue"/>
		{{/browserLoaded}}
	</div>
	
	</div>
	</div>
</div>

{{#if showTerms}}
	<div class='centerVertical modalBackground' style=''>
		<div class='modalInset firstUser'>
			<div class='headline'>
				{{systemProdName}} Terms and Conditions <span class='inlineButton' style='margin-left:10vw;' ($click)='activatePrint(%element, %event)'>PRINT</span>
					{{{closingBox}}}
			</div>
			<div id='termsModalText'>
			<div>
		</div>
	</div>
{{/if}}
<!-- account/login end -->

