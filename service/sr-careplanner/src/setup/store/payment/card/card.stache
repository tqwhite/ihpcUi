
<input class="c-field" placeholder="Name on Card" {($value)}="ccInfo.name" fieldName='name'>
<input class="c-field" placeholder="Card Number (spaces optional)" {($value)}="ccInfo.number" fieldName='number'>
<input class="c-field" placeholder="Expire MM" {($value)}="ccInfo.expMonth" fieldName='expMonth' style='width:47%;display:inline-block;'>
<input class="c-field" placeholder="Expire YY" {($value)}="ccInfo.expYear" fieldName='expYear' style='width:47%;display:inline-block;'><br/>
<input class="c-field" placeholder="Code on Card Back" {($value)}="ccInfo.cardCode" fieldName='cardCode'style='width:47%;display:inline-block;'>

<input class="c-field" placeholder="Card Zip Code" {($value)}="ccInfo.zip" fieldName='zip' style='width:47%;display:inline-block;'>